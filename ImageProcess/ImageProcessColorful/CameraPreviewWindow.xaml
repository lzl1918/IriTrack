<Window x:Class="ImageProcess.CamerPreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageProcess"
        mc:Ignorable="d"
        Title="ResultWindow" Height="300" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image Name="img_src"/>
        <Image Name="img_dst" Grid.Column="1"/>
        <TextBlock Name="textFPS" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="8" FontSize="32" TextAlignment="Right"/>
    </Grid>
</Window>
